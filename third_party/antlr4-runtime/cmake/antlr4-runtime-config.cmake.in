# ANTLR4 Runtime Config File
@PACKAGE_INIT@

include("${CMAKE_CURRENT_LIST_DIR}/antlr4-runtime-targets.cmake")

set(ANTLR4_INCLUDE_DIR "@CMAKE_INSTALL_PREFIX@/include/antlr4-runtime")

# Provide variables for compatibility
set(ANTLR4_INCLUDE_DIRS ${ANTLR4_INCLUDE_DIR})
set(ANTLR4_LIBRARIES antlr4::antlr4-runtime)

# Check required components
include(FindPackageHandleStandardArgs)
find_package_handle_standard_args(antlr4-runtime
    REQUIRED_VARS ANTLR4_INCLUDE_DIR
    VERSION_VAR PACKAGE_VERSION
)