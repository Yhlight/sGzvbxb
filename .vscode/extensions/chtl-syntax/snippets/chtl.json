{
    "CHTL Declaration Block": {
        "prefix": "[",
        "body": "[${1|Template,Custom,Import,Namespace,Configuration,Info,Export,Origin|}] $0",
        "description": "CHTL declaration block with brackets"
    },
    "Quick Template": {
        "prefix": "[t",
        "body": "[Template] @${1|Style,Element,Var|} ${2:Name}\n{\n    $0\n}",
        "description": "Quick template declaration"
    },
    "Quick Custom": {
        "prefix": "[c",
        "body": "[Custom] @${1|Style,Element,Var|} ${2:Name}\n{\n    $0\n}",
        "description": "Quick custom declaration"
    },
    "Quick Import": {
        "prefix": "[i",
        "body": "[Import] @${1|Chtl,Html,Style,JavaScript,CJmod|} from ${2:path}",
        "description": "Quick import statement"
    },
    "Quick Namespace": {
        "prefix": "[n",
        "body": "[Namespace] ${1:NamespaceName}\n{\n    $0\n}",
        "description": "Quick namespace declaration"
    },
    "Quick Origin": {
        "prefix": "[o",
        "body": "[Origin] @${1|Html,Style,JavaScript|}\n{\n    $0\n}",
        "description": "Quick origin block"
    },
    "HTML Template": {
        "prefix": "html",
        "body": [
            "html",
            "{",
            "    head",
            "    {",
            "        meta { charset: \"UTF-8\"; }",
            "        title: \"${1:Page Title}\";",
            "        $0",
            "    }",
            "    ",
            "    body",
            "    {",
            "        ",
            "    }",
            "}"
        ],
        "description": "Basic HTML structure"
    },
    "Template Style": {
        "prefix": "template-style",
        "body": [
            "[Template] @Style ${1:StyleName}",
            "{",
            "    ${2:property}: ${3:value};",
            "    $0",
            "}"
        ],
        "description": "Create a style template"
    },
    "Template Element": {
        "prefix": "template-element",
        "body": [
            "[Template] @Element ${1:ElementName}",
            "{",
            "    ${2:div}",
            "    {",
            "        $0",
            "    }",
            "}"
        ],
        "description": "Create an element template"
    },
    "Template Variable": {
        "prefix": "template-var",
        "body": [
            "[Template] @Var ${1:VarGroupName}",
            "{",
            "    ${2:varName}: \"${3:value}\";",
            "    $0",
            "}"
        ],
        "description": "Create a variable group template"
    },
    "Custom Style": {
        "prefix": "custom-style",
        "body": [
            "[Custom] @Style ${1:StyleName}",
            "{",
            "    ${2:property},",
            "    ${3:property};",
            "    $0",
            "}"
        ],
        "description": "Create a custom style group"
    },
    "Import CHTL": {
        "prefix": "import",
        "body": [
            "[Import] @${1|Chtl,Html,Style,JavaScript|} from ${2:path}"
        ],
        "description": "Import statement"
    },
    "Import Module": {
        "prefix": "import-module",
        "body": [
            "[Import] @Chtl from ${1:ModuleName}"
        ],
        "description": "Import CHTL module"
    },
    "Namespace": {
        "prefix": "namespace",
        "body": [
            "[Namespace] ${1:NamespaceName}",
            "{",
            "    $0",
            "}"
        ],
        "description": "Create namespace"
    },
    "Style Block": {
        "prefix": "style",
        "body": [
            "style",
            "{",
            "    ${1:property}: ${2:value};",
            "    $0",
            "}"
        ],
        "description": "Style block"
    },
    "Script Block": {
        "prefix": "script",
        "body": [
            "script",
            "{",
            "    $0",
            "}"
        ],
        "description": "Script block"
    },
    "Enhanced Selector": {
        "prefix": "selector",
        "body": [
            "{{${1:.className}}}$0"
        ],
        "description": "CHTL JS enhanced selector"
    },
    "Listen Method": {
        "prefix": "listen",
        "body": [
            "{{${1:selector}}}->listen({",
            "    ${2:click}: () => {",
            "        $0",
            "    }",
            "});"
        ],
        "description": "CHTL JS listen method"
    },
    "Delegate Method": {
        "prefix": "delegate",
        "body": [
            "{{${1:parent}}}->delegate({",
            "    target: {{${2:selector}}},",
            "    ${3:click}: (e) => {",
            "        $0",
            "    }",
            "});"
        ],
        "description": "CHTL JS delegate method"
    },
    "Animate Function": {
        "prefix": "animate",
        "body": [
            "let ${1:animation} = animate({",
            "    duration: ${2:300},",
            "    easing: '${3:ease-in-out}',",
            "    begin: { ${4:opacity: 0} },",
            "    end: { ${5:opacity: 1} }",
            "});",
            "",
            "${1:animation}({{${6:element}}});"
        ],
        "description": "CHTL JS animate function"
    },
    "Text Node": {
        "prefix": "text",
        "body": [
            "text { \"${1:content}\" }"
        ],
        "description": "Text node"
    },
    "Div Element": {
        "prefix": "div",
        "body": [
            "div",
            "{",
            "    ${1:class}: \"${2:className}\";",
            "    $0",
            "}"
        ],
        "description": "Div element"
    },
    "Button Element": {
        "prefix": "button",
        "body": [
            "button",
            "{",
            "    text { \"${1:Click me}\" }",
            "    ",
            "    script",
            "    {",
            "        {{button}}->listen({",
            "            click: () => {",
            "                $0",
            "            }",
            "        });",
            "    }",
            "}"
        ],
        "description": "Button with click handler"
    },
    "Chtholly Import": {
        "prefix": "chtholly",
        "body": [
            "[Import] @Chtl from Chtholly",
            "",
            "// 使用珂朵莉主题",
            "div",
            "{",
            "    style",
            "    {",
            "        @Style Chtholly.${1:FairyCard};",
            "    }",
            "    $0",
            "}"
        ],
        "description": "Import and use Chtholly module"
    }
}